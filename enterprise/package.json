    "docker": "^1.0.0",
    "kubernetes": "^0.13.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quantlink/oracle-contracts.git",
