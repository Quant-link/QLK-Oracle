{
  "name": "@quantlink/enterprise-integration",
  "version": "1.0.0",
  "description": "Enterprise integration suite for QuantLink Oracle with traditional finance compatibility",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run build:docs",
    "build:docs": "typedoc --out docs src",
    "start": "node dist/index.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:build": "docker build -t quantlink-enterprise .",
    "docker:run": "docker run -p 8080:8080 quantlink-enterprise",
    "k8s:deploy": "kubectl apply -f k8s/",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "generate:api-docs": "swagger-jsdoc -d swagger.config.js -o docs/api.json src/**/*.ts",
    "generate:sdk": "npm run generate:sdk:python && npm run generate:sdk:js && npm run generate:sdk:java && npm run generate:sdk:go",
    "generate:sdk:python": "openapi-generator-cli generate -i docs/api.json -g python -o sdks/python",
    "generate:sdk:js": "openapi-generator-cli generate -i docs/api.json -g javascript -o sdks/javascript",
    "generate:sdk:java": "openapi-generator-cli generate -i docs/api.json -g java -o sdks/java",
    "generate:sdk:go": "openapi-generator-cli generate -i docs/api.json -g go -o sdks/go"
  },
  "keywords": [
    "oracle",
    "blockchain",
    "enterprise",
    "api",
    "finance",
    "integration",
    "quantlink"
  ],
  "author": "QuantLink Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "apollo-server-express": "^3.12.1",
    "graphql": "^16.8.1",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.7.0",
    "passport-saml": "^3.2.4",
    "passport-local": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3",
    "kafkajs": "^2.2.4",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "knex": "^3.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "prom-client": "^15.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "axios": "^1.6.2",
    "ws": "^8.14.2",
    "socket.io": "^4.7.4",
    "multer": "^1.4.5-lts.1",
    "csv-parser": "^3.0.0",
    "json2csv": "^6.1.0",
    "xlsx": "^0.18.5",
    "pdf-lib": "^1.17.1",
    "nodemailer": "^6.9.7",
    "cron": "^3.1.6",
    "node-cache": "^5.1.2",
    "express-validator": "^7.0.1",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "bull": "^4.12.2",
    "bull-board": "^2.1.3",
    "elastic-apm-node": "^4.5.2",
    "newrelic": "^11.7.0",
    "datadog-metrics": "^0.9.3",
    "opentelemetry": "^0.1.4",
    "jaeger-client": "^3.19.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/passport": "^1.0.16",
    "@types/passport-oauth2": "^1.4.15",
    "@types/passport-local": "^1.0.38",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/speakeasy": "^2.0.10",
    "@types/qrcode": "^1.5.5",
    "@types/pg": "^8.10.7",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.9",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "@types/cron": "^2.0.1",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "prettier": "^3.1.0",
    "typedoc": "^0.25.4",
